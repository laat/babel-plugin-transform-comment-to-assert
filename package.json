{
  "name": "babel-plugin-transform-comment-to-assert",
  "version": "4.1.0",
  "description": "Replace commented expressions with assert statements",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel src/. -d lib/. --ignore=spec.js",
    "test:spec": "babel-node src/index.spec.js",
    "#test:readme": "readme-assert --main './src'",
    "test": "npm-run-all test:*",
    "prepublish": "npm run build",
    "precommit": "lint-staged"
  },
  "author": {
    "name": "Sigurd Fosseng",
    "email": "sigurd@fosseng.net",
    "url": "http://laat.io"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/laat/babel-plugin-transform-comment-to-assert.git"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@types/babel__core": "^7.1.2",
    "assert-simple-tap": "^2.0.1",
    "eslint": "^4.6.1",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-config-prettier": "^2.4.0",
    "eslint-plugin-import": "^2.2.0",
    "lint-staged": "^4.1.3",
    "npm-run-all": "^4.0.2",
    "prettier": "^1.6.1",
    "readme-assert": "^5.0.5"
  },
  "files": [
    "lib"
  ],
  "dependencies": {
    "@babel/helper-plugin-utils": "^7.0.0"
  }
}
