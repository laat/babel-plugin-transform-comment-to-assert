{
  "name": "babel-plugin-transform-comment-to-assert",
  "version": "3.0.1",
  "description": "Replace commented expressions with assert statements",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel src/. -d lib/. --ignore=spec.js",
    "test:spec": "babel-node src/index.spec.js",
    "test:readme": "readme-assert --main './src' --require babel-register",
    "test": "npm-run-all lint test:*",
    "lint": "eslint src",
    "prepublish": "npm run build"
  },
  "author": {
    "name": "Sigurd Fosseng",
    "email": "sigurd@fosseng.net",
    "url": "http://laat.io"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/laat/babel-plugin-transform-comment-to-assert.git"
  },
  "devDependencies": {
    "assert-simple-tap": "^1.0.2",
    "babel-cli": "^6.4.0",
    "babel-core": "^6.14.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.4.3",
    "eslint": "^3.4.0",
    "eslint-config-airbnb-base": "^7.0.0",
    "eslint-plugin-import": "^1.14.0",
    "npm-run-all": "^3.1.0",
    "readme-assert": "^5.0.0"
  },
  "files": [
    "lib"
  ]
}
