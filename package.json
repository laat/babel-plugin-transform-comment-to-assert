{
  "name": "babel-plugin-transform-comment-to-assert",
  "version": "1.0.7",
  "description": "Replace commented expressions with assert statements",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rm -rf lib",
    "build": "babel src/. -d lib/. --ignore=spec.js",
    "test:spec": "mocha --require babel-register --reporter mocha-tap13 src/**/*.spec.js",
    "test:readme": "readme-assert --main './src' --require babel-register",
    "test": "npm run test:readme && npm run test:spec",
    "lint": "eslint src",
    "jscs": "jscs src",
    "prebuild": "npm run clean",
    "prepublish": "npm run build"
  },
  "author": {
    "name": "Sigurd Fosseng",
    "email": "sigurd@fosseng.net",
    "url": "http://laat.io"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/laat/babel-plugin-transform-comment-to-assert.git"
  },
  "devDependencies": {
    "babel-cli": "^6.4.0",
    "babel-core": "^6.4.5",
    "babel-generator": "^6.4.5",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.4.3",
    "chai": "^3.4.1",
    "eslint": "^2.8.0",
    "eslint-config-airbnb": "^7.0.0",
    "eslint-plugin-import": "^1.5.0",
    "jscs": "^3.0.3",
    "mocha": "^2.3.4",
    "mocha-tap13": "0.0.3",
    "readme-assert": "^2.1.2",
    "standard": "^6.0.8"
  },
  "files": [
    "lib"
  ],
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "dependencies": {
    "babel-template": "^6.3.13",
    "babel-traverse": "^6.4.5",
    "babel-types": "^6.4.5",
    "babylon": "^6.4.5"
  }
}
